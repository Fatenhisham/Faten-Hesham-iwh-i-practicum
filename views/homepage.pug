doctype html
html
  head
    title= title
  body
    h1 Pets Custom Object

    a(href="/update-cobj") Add to this table
    br
    br

    table(border="1")
      tr
        th Name
        th Type
        th Age

      each pet in pets
        tr
          td= pet.properties.name
          td= pet.properties.type
          td= pet.properties.age
